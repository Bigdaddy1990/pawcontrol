{
  "domain": "pawcontrol",
  "name": "Paw Control",
  "version": "1.0.0",
  "documentation": "https://github.com/BigDaddy1990/pawcontrol",
  "issue_tracker": "https://github.com/BigDaddy1990/pawcontrol/issues",
  "dependencies": [],
  "after_dependencies": [
    "mobile_app",
    "person",
    "zone",
    "device_tracker",
    "bluetooth",
    "usb",
    "zeroconf",
    "dhcp"
  ],
  "requirements": [
    "aiofiles>=23.1.0",
    "asyncio-mqtt>=0.16.0"
  ],
  "config_flow": true,
  "codeowners": [
    "@BigDaddy1990"
  ],
  "dhcp": [
    {
      "hostname": "tractive-*",
      "macaddress": "00:11:22:*"
    },
    {
      "hostname": "petnet-*",
      "macaddress": "44:55:66:*"
    },
    {
      "hostname": "whistle-*",
      "macaddress": "AA:BB:CC:*"
    }
  ],
  "bluetooth": [
    {
      "service_uuid": "0000180f-0000-1000-8000-00805f9b34fb",
      "connectable": true
    }
  ],
  "usb": [
    {
      "vid": "1234",
      "pid": "5678",
      "description": "tractive gps tracker"
    },
    {
      "vid": "2345",
      "pid": "6789",
      "description": "whistle health monitor"
    }
  ],
  "zeroconf": [
    {
      "type": "_pawcontrol._tcp.local.",
      "name": "paw-control-*"
    },
    {
      "type": "_petnet._tcp.local.",
      "name": "petnet-*"
    },
    {
      "type": "_tractive._tcp.local.",
      "name": "tractive-*"
    }
  ],
  "integration_type": "hub",
  "iot_class": "local_push",
  "quality_scale": "platinum",
  "loggers": [
    "custom_components.pawcontrol"
  ],
  "homekit": {
    "models": [
      "tractive gps tracker",
      "petnet smart feeder",
      "whistle health monitor"
    ]
  }
}
