{
  "domain": "pawcontrol",
  "name": "PawControl",
  "after_dependencies": [
    "bluetooth",
    "device_tracker",
    "mqtt",
    "person",
    "usb",
    "zone"
  ],
  "bluetooth": [
    {
      "local_name": "PawControl*"
    }
  ],
  "codeowners": [
    "@BigDaddy1990"
  ],
  "config_flow": true,
  "dependencies": [],
  "dhcp": [
    {
      "hostname": "pawcontrol-*"
    }
  ],
  "documentation": "https://github.com/BigDaddy1990/pawcontrol",
  "homeassistant": "2026.2.0",
  "integration_type": "hub",
  "iot_class": "local_push",
  "issue_tracker": "https://github.com/bigdaddy1990/pawcontrol/issues",
  "loggers": [
    "custom_components.pawcontrol"
  ],
  "quality_scale": "platinum",
  "requirements": [
    "aiofiles>=24.1.0"
  ],
  "single_config_entry": true,
  "usb": [
    {
      "manufacturer": "pawcontrol"
    }
  ],
  "version": "1.0.0",
  "zeroconf": [
    {
      "type": "_pawcontrol._tcp.local."
    }
  ]
}
